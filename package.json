{
  "name": "automai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.config.e2e.js",
    "server:dev": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "server:build": "tsc -p src/server/tsconfig.json",
    "server:start": "node dist/server/index.js",
    "prisma:generate": "prisma generate --schema=src/server/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/server/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/server/prisma/schema.prisma",
    "prisma:seed": "ts-node src/server/prisma/seed.ts"
  },
  "dependencies": {
