{
  "name": "automai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:e2e": "NODE_ENV=test jest --config jest.config.e2e.js",
    "server:dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/server/index.ts",
    "server:prod": "NODE_ENV=production ts-node-dev --respawn --transpile-only src/server/index.ts",
    "server:test": "NODE_ENV=test ts-node-dev --respawn --transpile-only src/server/index.ts",
    "server:build": "tsc -p src/server/tsconfig.json",
    "server:start": "NODE_ENV=production node dist/server/index.js",
    "prisma:generate": "prisma generate --schema=src/server/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/server/prisma/schema.prisma",
    "prisma:migrate:prod": "prisma migrate deploy --schema=src/server/prisma/schema.prisma",
    "prisma:studio": "DATABASE_URL=\"postgresql://joachimndoye@localhost:5432/automai_db\" prisma studio --schema=src/server/prisma/schema.prisma",
    "prisma:seed": "NODE_ENV=development ts-node src/server/prisma/seed.ts",
    "prisma:seed:test": "NODE_ENV=test ts-node src/server/prisma/seed.ts",
    "delete-user": "cd src/server && DATABASE_URL=postgresql://postgres:postgres@localhost:5432/automai npx ts-node -e \"const { PrismaClient } = require('@prisma/client'); const prisma = new PrismaClient(); (async () => { try { await prisma.user.delete({ where: { email: 'joachim_djibril@hotmail.com' } }); console.log('User deleted'); } catch (e) { console.error(e); } finally { await prisma.$disconnect(); } })();\"",
    "electron-dev": "cross-env ELECTRON_PORT=3000 electron electron/main.js",
    "electron-build": "next build && electron-builder",
    "electron-pack": "npm run build && electron-builder -c.extraMetadata.main=electron/main.js"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@prisma/client": "^6.4.1",
    "@puppeteer/browsers": "^2.7.1",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@supabase/supabase-js": "^2.48.1",
    "@tabler/icons-react": "^3.30.0",
    "@tanstack/react-table": "^8.21.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "electron-store": "^10.0.1",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.475.0",
    "next": "latest",
    "next-auth": "^4.24.11",
    "next-intl": "^3.26.4",
    "next-themes": "^0.4.4",
    "node-ssh": "^13.2.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^6.4.1",
    "react": "latest",
    "react-dom": "latest",
    "recharts": "^2.15.1",
    "shadcn-ui": "^0.9.4",
    "simple-git": "^3.27.0",
    "tailwind-merge": "^3.0.1",
    "ts-node": "^10.9.2",
    "xterm": "^5.3.0"
  },
  "devDependencies": {
    "@shadcn/ui": "^0.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "jest-environment-puppeteer": "^9.0.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "puppeteer": "^22.15.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "wait-on": "^8.0.2"
  },
  "prisma": {
    "schema": "src/server/prisma/schema.prisma",
    "seed": "ts-node src/server/prisma/seed.ts"
  },
  "build": {
    "appId": "com.automai.app",
    "productName": "AutomAI",
    "files": [
      "electron/**/*",
      "out/**/*",
      "public/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
