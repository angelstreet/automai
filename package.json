{
  "name": "automai",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "NODE_ENV=development ts-node server.ts",
    "dev:server": "ts-node server.ts",
    "dev:next": "next dev",
    "dev:all": "node scripts/dev.js",
    "dev:debug": "node scripts/debug.js",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:next": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "NODE_ENV=test jest --config jest.config.e2e.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy --schema=prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=prisma/schema.prisma",
    "prisma:seed": "NODE_ENV=development ts-node prisma/seed.ts",
    "prisma:seed:test": "NODE_ENV=test ts-node prisma/seed.ts",
    "delete-user": "DATABASE_URL=postgresql://postgres:postgres@localhost:5432/automai npx ts-node -e \"const { PrismaClient } = require('@prisma/client'); const prisma = new PrismaClient(); (async () => { try { await prisma.user.delete({ where: { email: 'joachim_djibril@hotmail.com' } }); console.log('User deleted'); } catch (e) { console.error(e); } finally { await prisma.$disconnect(); } })();\"",
    "electron-dev": "cross-env ELECTRON_PORT=3000 electron electron/main.js",
    "electron-build": "next build && electron-builder",
    "electron-pack": "npm run build && electron-builder -c.extraMetadata.main=electron/main.js",
    "update-deps": "npm update && npm outdated"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.8.0",
    "@hookform/resolvers": "4.1.2",
    "@monaco-editor/react": "^4.7.0",
    "@prisma/client": "^6.4.1",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@radix-ui/react-visually-hidden": "^1.1.2",
    "@t3-oss/env-nextjs": "0.7.1",
    "@tabler/icons-react": "^3.30.0",
    "@tanstack/react-query": "5.66.11",
    "@tanstack/react-table": "^8.21.2",
    "@types/bcrypt": "^5.0.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/next": "9.0.0",
    "@xterm/addon-attach": "^0.12.0-beta.98",
    "@xterm/addon-fit": "^0.11.0-beta.98",
    "@xterm/addon-search": "^0.16.0-beta.98",
    "@xterm/addon-web-links": "^0.12.0-beta.98",
    "@xterm/xterm": "^5.6.0-beta.98",
    "bcrypt": "^5.1.1",
    "chart.js": "4.4.8",
    "clsx": "2.1.1",
    "cmdk": "^1.0.4",
    "crypto-browserify": "^3.12.1",
    "date-fns": "4.1.0",
    "dotenv": "^16.4.7",
    "electron-store": "^10.0.1",
    "is-electron": "^2.2.2",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.2.0",
    "next-auth": "^4.24.7",
    "next-intl": "3.26.5",
    "prisma": "^6.4.1",
    "react": "latest",
    "react-chartjs-2": "5.3.0",
    "react-dom": "latest",
    "simple-git": "^3.27.0",
    "sonner": "2.0.1",
    "ssh2": "^1.16.0",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "3.0.2",
    "ts-node": "^10.9.2",
    "ws": "^8.18.1",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.0",
    "@eslint/js": "^9.21.0",
    "@humanwhocodes/module-importer": "^1.0.1",
    "@next/bundle-analyzer": "^15.2.0",
    "@tanstack/react-router": "^1.112.0",
    "@types/node": "^22.13.5",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/ssh2": "^1.15.4",
    "@types/ws": "8.5.14",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.25.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "0.7.1",
    "cross-env": "^7.0.3",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.2.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "lru-cache": "^10.2.0",
    "lucide-react": "0.477.0",
    "next-themes": "0.4.4",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "puppeteer": "^22.15.0",
    "react-hook-form": "^7.54.2",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "1.0.7",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node prisma/seed.ts"
  },
  "build": {
    "appId": "com.automai.app",
    "productName": "AutomAI",
    "files": [
      "electron/**/*",
      "out/**/*",
      "public/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "description": "Automai is a multi-tenant SaaS platform designed for end-to-end test automation across web, desktop, and mobile environments.",
  "main": "jest.config.e2e.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angelstreet/automai.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/angelstreet/automai/issues"
  },
  "homepage": "https://github.com/angelstreet/automai#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}
