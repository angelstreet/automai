    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.475.0",
    "next": "latest",
    "next-intl": "^3.26.4",
    "next-themes": "^0.4.4",
    "prisma": "^6.4.1",
    "react": "latest",
    "react-dom": "latest",
    "tailwind-merge": "^3.0.1",
    "ts-node": "^10.9.2"
